{
    "name": "jakobmats/empress",
    "description": "Small library built on top of amphp/http-server for creating async web apps.",
    "type": "project",
    "require": {
        "amphp/amp": "^2.1",
        "amphp/http-server": "^1.1",
        "monolog/monolog": "^1.24",
        "amphp/log": "^1.0",
        "amphp/http-server-router": "^1.0",
        "pimple/pimple": "^3.2",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Empress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Empress\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "amphp/phpunit-util": "^1.1"
    }
}