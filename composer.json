{
    "name": "jakobmats/empress",
    "description": "Small library built on top of amphp/http-server for creating async web apps.",
    "type": "project",
    "require": {
        "amphp/amp": "^2.1",
        "amphp/http-server": "^1.1",
        "monolog/monolog": "^1.24",
        "amphp/log": "^1.0",
        "amphp/http-server-router": "^1.0",
        "psr/container": "^1.0",
        "amphp/file": "^0.3.5",
        "amphp/http-server-form-parser": "^1.1",
        "amphp/http-server-static-content": "^1.0",
        "symfony/polyfill-php73": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Empress\\": "src/"
        },
        "files": [
            "src/Routing/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Empress\\Test\\": "test/"
        }
    },
    "require-dev": {
        "amphp/artax": "^3.0",
        "psy/psysh": "^0.9.9",
        "phpunit/phpunit": "^8.3",
        "amphp/phpunit-util": "^1.1"
    }
}
