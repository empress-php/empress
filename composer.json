{
    "name": "jakobmats/empress",
    "description": "Small library built on top of amphp/http-server for creating async web apps.",
    "type": "project",
    "require": {
        "amphp/amp": "^2.1",
        "amphp/http-server": "^1.1",
        "monolog/monolog": "^1.24",
        "amphp/log": "^1.0",
        "amphp/http-server-router": "^1.0",
        "pimple/pimple": "^3.2",
        "psr/container": "^1.0",
        "amphp/file": "^0.3.5",
        "amphp/http-server-form-parser": "^1.1",
        "symfony/dependency-injection": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/config": "^4.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cspray/phpunit-util"
        }
    ],
    "autoload": {
        "psr-4": {
            "Empress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Empress\\Test\\": "test/"
        }
    },
    "require-dev": {
        "amphp/phpunit-util": "dev-feature/phpunit-7",
        "amphp/artax": "^3.0"
    }
}
